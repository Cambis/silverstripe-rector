{
  "name": "cambis/silverstripe-rector",
  "description": "Rector rules for Silverstripe.",
  "license": "MIT",
  "keywords": [
      "silverstripe",
      "rector",
      "dev"
  ],
  "require": {
      "php": "^7.4 || ^8.0",
      "cambis/silverstan": "^2.1",
      "rector/rector": "^2.1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
      "psr-4": {
          "Cambis\\SilverstripeRector\\": [
              "src/",
              "rules/"
          ]
      }
  },
  "config": {
      "allow-plugins": {
          "composer/installers": true,
          "silverstripe/vendor-plugin": true
      },
      "sort-packages": true
  }
}
