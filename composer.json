{
  "name": "cambis/silverstripe-rector",
  "description": "Rector rules for Silverstripe CMS.",
  "license": "MIT",
  "keywords": [
    "silverstripe",
    "rector",
    "dev"
  ],
  "authors": [
    {
      "name": "Cameron Bryers"
    }
  ],
  "homepage": "https://github.com/Cambis/silverstripe-rector",
  "require": {
    "php": "^7.4 || ^8.0",
    "cambis/silverstan": "^2.1.7",
    "rector/rector": "^2.1.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Cambis\\SilverstripeRector\\": [
        "src/",
        "rules/"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    },
    "sort-packages": true
  }
}
